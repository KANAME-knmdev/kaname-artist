{
  "name": "kaname-artist",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://kaname-artist.web.app",
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.11",
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "@material-ui/lab": "^4.0.0-alpha.17",
    "@material-ui/styles": "^4.1.1",
    "emotion-theming": "^10.0.10",
    "eslint-plugin-react": "^7.14.2",
    "firebase": "^6.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-firebase-hooks": "^2.0.0",
    "react-hook-form": "^3.11.9",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "use-react-router": "^1.0.7"
  },
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/node": "12.0.10",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "@typescript-eslint/parser": "^1.9.0",
    "cross-env": "^5.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react-app": "^5.0.1",
    "husky": "^2.4.1",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  },
  "scripts": {
    "start": "react-scripts start -p 3000",
    "mock": "cross-env REACT_APP_MOCK=1 REACT_APP_LANG=Ja react-scripts start -p 3000",
    "mock-server": "cd mock && json-server -p 3001 --watch db.json",
    "build": "react-scripts build",
    "lint": "eslint --rule \"no-unused-vars: \"error\"\" --color \"./src/**/*.{js,jsx,ts,tsx,json}\" --fix",
    "test": "react-scripts test",
    "deploy": "yarn build && firebase deploy"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  }
}
